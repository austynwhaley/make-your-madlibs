<div data-aos="zoom-in-down" class="jumbotron">
  <img class="img-fluid rounded mx-auto d-block max-width: 100%;" src="./images/libarch.png" alt="">
</div>

{{#each stories}}
  <div class="row mb-5">
    <div class="col-2"></div>
    <div class="col-10">
      <div data-aos="flip-up" class="card" style="width: 75%;" >
        <div class="card-body">
          <h5 class="card-title">{{this.title}}</h5>
          <p>{{this.storyBody}}</p>
        </div>
      </div>
    </div>
  </div>
{{/each}}
    
